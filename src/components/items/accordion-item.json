{
  "collectionName": "components_items_accordion_items",
  "info": {
    "displayName": "Accordion Item",
    "icon": "filter"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "linkList": {
      "type": "component",
      "repeatable": true,
      "component": "items.video-item"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "uniqueInUruguay": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "image": {
      "type": "component",
      "repeatable": false,
      "component": "items.image-item"
    }
  }
}
